{
    "name": "kamilmusial/ntfy-php-bundle",
    "description": "Symfony bundle for kamilmusial/ntfy-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamil Musial",
            "email": "kamil.k.musial@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "kamilmusial/ntfy-php": "^0.1",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0",
        "symfony/http-kernel": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Kamilmusial\\NtfyPhpBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kamilmusial\\NtfyPhpBundleTest\\": "tests"
        }
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.8",
        "phpstan/phpstan": "^1.10",
        "slevomat/coding-standard": "^8.14",
        "spaze/phpstan-disallowed-calls": "^3.0"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
